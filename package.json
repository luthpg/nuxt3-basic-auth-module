{
  "name": "@luthpg/nuxt3-basic-auth-module",
  "version": "0.1.0",
  "description": "Basic-Auth module for Nuxt3",
  "repository": "https://github.com/luthpg/nuxt3-basic-auth-module.git",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/luthpg"
  },
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && pnpm exec playwright install",
    "release:prepare": "pnpm run lint && pnpm run test && pnpm run prepack && changelogen --bump",
    "release": "pnpm release:prepare && pnpm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepublishOnly": "pnpm dev:prepare && pnpm release:prepare"
  },
  "dependencies": {
    "@nuxt/kit": "^3.10.3"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.10.3",
    "@nuxt/test-utils": "^3.11.0",
    "@types/node": "^20.11.25",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "nuxt": "^3.10.3",
    "playwright": "^1.42.1",
    "vitest": "^1.3.1"
  }
}